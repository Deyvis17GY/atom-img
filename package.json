{
  "name": "@deyvis17gy/atom-img",
  "version": "1.0.33",
  "description": "image preloading for slow or large connections",
  "author": "Deyvis17GY",
  "license": "MIT",
  "repository": "https://github.com/Deyvis17GY/atom-img.git",
  "homepage": "https://deyvis17gy.github.io/atom-img/",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "ui",
    "react",
    "library",
    "design system"
  ],
  "scripts": {
    "build": "rimraf --no-glob ./dist && microbundle --jsx React.createElement --tsconfig ./tsconfig.json -f es,cjs --no-compress",
    "start": "microbundle-crl watch --no-compress --no-css-modules --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "deploy:netlify:storybook": "build-storybook && netlify deploy --dir storybook-static --prod",
    "npm:login": "npm login",
    "publish:npm": "npm publish --access public"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^16.0.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/plugin-transform-regenerator": "^7.18.6",
    "@babel/preset-modules": "^0.1.5",
    "@storybook/addon-actions": "6.5.8",
    "@storybook/addon-essentials": "6.5.8",
    "@storybook/addon-interactions": "6.5.8",
    "@storybook/addon-links": "6.5.8",
    "@storybook/builder-webpack4": "6.5.8",
    "@storybook/manager-webpack4": "6.5.8",
    "@storybook/node-logger": "6.5.8",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.5.8",
    "@types/react": "^18.2.20",
    "@types/storybook__react": "5.2.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^3.4.1",
    "sass": "^1.52.3",
    "sass-loader": "^13.0.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {},
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
